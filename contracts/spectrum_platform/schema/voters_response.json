{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "VotersResponse",
  "type": "object",
  "required": [
    "voters"
  ],
  "properties": {
    "voters": {
      "type": "array",
      "items": {
        "type": "array",
        "items": [
          {
            "$ref": "#/definitions/HumanAddr"
          },
          {
            "$ref": "#/definitions/VoterInfo"
          }
        ],
        "maxItems": 2,
        "minItems": 2
      }
    }
  },
  "definitions": {
    "HumanAddr": {
      "type": "string"
    },
    "VoteOption": {
      "type": "string",
      "enum": [
        "yes",
        "no"
      ]
    },
    "VoterInfo": {
      "type": "object",
      "required": [
        "balance",
        "vote"
      ],
      "properties": {
        "balance": {
          "type": "integer",
          "format": "uint32",
          "minimum": 0.0
        },
        "vote": {
          "$ref": "#/definitions/VoteOption"
        }
      }
    }
  }
}
