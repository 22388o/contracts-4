{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ConfigInfo",
  "type": "object",
  "required": [
    "effective_delay",
    "expiration_period",
    "mint_end",
    "mint_per_block",
    "mint_start",
    "owner",
    "proposal_deposit",
    "quorum",
    "threshold",
    "voting_period",
    "warchest_ratio"
  ],
  "properties": {
    "effective_delay": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "expiration_period": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "mint_end": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "mint_per_block": {
      "$ref": "#/definitions/Uint128"
    },
    "mint_start": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "owner": {
      "$ref": "#/definitions/HumanAddr"
    },
    "proposal_deposit": {
      "$ref": "#/definitions/Uint128"
    },
    "quorum": {
      "$ref": "#/definitions/Decimal"
    },
    "spec_token": {
      "anyOf": [
        {
          "$ref": "#/definitions/HumanAddr"
        },
        {
          "type": "null"
        }
      ]
    },
    "threshold": {
      "$ref": "#/definitions/Decimal"
    },
    "voting_period": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "warchest_address": {
      "anyOf": [
        {
          "$ref": "#/definitions/HumanAddr"
        },
        {
          "type": "null"
        }
      ]
    },
    "warchest_ratio": {
      "$ref": "#/definitions/Decimal"
    }
  },
  "definitions": {
    "Decimal": {
      "description": "A fixed-point decimal value with 18 fractional digits, i.e. Decimal(1_000_000_000_000_000_000) == 1.0\n\nThe greatest possible value that can be represented is 340282366920938463463.374607431768211455 (which is (2^128 - 1) / 10^18)",
      "type": "string"
    },
    "HumanAddr": {
      "type": "string"
    },
    "Uint128": {
      "type": "string"
    }
  }
}
