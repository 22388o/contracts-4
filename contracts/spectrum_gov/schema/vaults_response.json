{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "VaultsResponse",
  "type": "object",
  "required": [
    "vaults"
  ],
  "properties": {
    "vaults": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/VaultInfo"
      }
    }
  },
  "definitions": {
    "HumanAddr": {
      "type": "string"
    },
    "VaultInfo": {
      "type": "object",
      "required": [
        "address",
        "weight"
      ],
      "properties": {
        "address": {
          "$ref": "#/definitions/HumanAddr"
        },
        "weight": {
          "type": "integer",
          "format": "uint32",
          "minimum": 0.0
        }
      }
    }
  }
}
