{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Cw20HookMsg",
  "anyOf": [
    {
      "type": "object",
      "required": [
        "poll_start"
      ],
      "properties": {
        "poll_start": {
          "type": "object",
          "required": [
            "description",
            "execute_msgs",
            "title"
          ],
          "properties": {
            "description": {
              "type": "string"
            },
            "execute_msgs": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/ExecuteMsg"
              }
            },
            "link": {
              "type": [
                "string",
                "null"
              ]
            },
            "title": {
              "type": "string"
            }
          }
        }
      }
    },
    {
      "type": "object",
      "required": [
        "stake_tokens"
      ],
      "properties": {
        "stake_tokens": {
          "type": "object",
          "properties": {
            "staker_addr": {
              "anyOf": [
                {
                  "$ref": "#/definitions/HumanAddr"
                },
                {
                  "type": "null"
                }
              ]
            }
          }
        }
      }
    }
  ],
  "definitions": {
    "ExecuteMsg": {
      "anyOf": [
        {
          "type": "object",
          "required": [
            "execute"
          ],
          "properties": {
            "execute": {
              "type": "object",
              "required": [
                "contract",
                "msg"
              ],
              "properties": {
                "contract": {
                  "$ref": "#/definitions/HumanAddr"
                },
                "msg": {
                  "type": "string"
                }
              }
            }
          }
        }
      ]
    },
    "HumanAddr": {
      "type": "string"
    }
  }
}
