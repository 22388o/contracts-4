{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ConfigInfo",
  "type": "object",
  "required": [
    "base_denom",
    "community_fee",
    "controller_fee",
    "deposit_fee",
    "lock_end",
    "lock_start",
    "mirror_gov",
    "mirror_staking",
    "mirror_token",
    "owner",
    "platform_fee",
    "spectrum_gov",
    "spectrum_token",
    "terraswap_factory"
  ],
  "properties": {
    "base_denom": {
      "type": "string"
    },
    "community_fee": {
      "$ref": "#/definitions/Decimal"
    },
    "controller": {
      "anyOf": [
        {
          "$ref": "#/definitions/HumanAddr"
        },
        {
          "type": "null"
        }
      ]
    },
    "controller_fee": {
      "$ref": "#/definitions/Decimal"
    },
    "deposit_fee": {
      "$ref": "#/definitions/Decimal"
    },
    "lock_end": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "lock_start": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "mirror_gov": {
      "$ref": "#/definitions/HumanAddr"
    },
    "mirror_staking": {
      "$ref": "#/definitions/HumanAddr"
    },
    "mirror_token": {
      "$ref": "#/definitions/HumanAddr"
    },
    "owner": {
      "$ref": "#/definitions/HumanAddr"
    },
    "platform": {
      "anyOf": [
        {
          "$ref": "#/definitions/HumanAddr"
        },
        {
          "type": "null"
        }
      ]
    },
    "platform_fee": {
      "$ref": "#/definitions/Decimal"
    },
    "spectrum_gov": {
      "$ref": "#/definitions/HumanAddr"
    },
    "spectrum_token": {
      "$ref": "#/definitions/HumanAddr"
    },
    "terraswap_factory": {
      "$ref": "#/definitions/HumanAddr"
    }
  },
  "definitions": {
    "Decimal": {
      "description": "A fixed-point decimal value with 18 fractional digits, i.e. Decimal(1_000_000_000_000_000_000) == 1.0\n\nThe greatest possible value that can be represented is 340282366920938463463.374607431768211455 (which is (2^128 - 1) / 10^18)",
      "type": "string"
    },
    "HumanAddr": {
      "type": "string"
    }
  }
}
